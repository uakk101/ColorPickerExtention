{

    "name" : "Color Picker",
    "description": "Color Picker Extention",
    "version": "1.0",
    "manifest_version" : 3,
    "background": {
        "service_worker" : "background.js"
    },

    "permissions":[
        "activeTab" ,
        "scripting"
    ],

    "action" : {
            "default_popup" : "popup.html"
    },

    "icons" : {
        "16" : "/images/dropper_16.png",
        "32" : "/images/dropper_32.png",
        "48" : "/images/dropper_48.png"
    }


}